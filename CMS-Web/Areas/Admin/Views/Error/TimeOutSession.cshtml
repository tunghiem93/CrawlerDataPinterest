@{
    Layout = "";
    ViewBag.Title = "TimeOut Session";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>NüWeb F&B | Timeout Session</title>

    <!-- Bootstrap -->
    @Styles.Render("~/Libaraies/vendors/bootstrap/dist/css/bootstrap.min.css")
    <!-- Font Awesome -->
    @Styles.Render("~/Libaraies/vendors/font-awesome/css/font-awesome.min.css")

    <!-- Custom Theme Style -->
    @Styles.Render("~/Content/css/custom.min.css")
</head>

<body class="nav-md" style="background-color:#fff !important;">
    <div class="container body">
        <div class="main_container">
            <!-- page content -->
            <div class="col-md-12">
                <div class="col-middle">
                    <div class="text-center text-center">
                        <h1 class="error-number" style="font-size:70px !important;">Timeout Session</h1>
                        <p>
                            Please [<a href="@Url.Action("Index","Login",new { area=""})">click here</a>] to go to Login page.
                        </p>
                    </div>
                </div>
            </div>
            <!-- /page content -->
        </div>
    </div>

    <!-- jQuery -->
    @Scripts.Render("~/Libaraies/vendors/jquery/dist/jquery.min.js")
    <!-- Bootstrap -->
    @Scripts.Render("~/Libaraies/vendors/bootstrap/dist/js/bootstrap.min.js")
    <!-- FastClick -->
    @Scripts.Render("~/Libaraies/vendors/fastclick/lib/fastclick.js")
    <!-- NProgress -->
    @Scripts.Render("~/Libaraies/vendors/nprogress/nprogress.js")

    <!-- Custom Theme Scripts -->
    @Scripts.Render("~/Scripts/js/custom.min.js")
</body>
</html>



